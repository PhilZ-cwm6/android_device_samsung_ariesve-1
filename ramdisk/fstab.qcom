# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

# INIT
#<src>                 <mnt_point>  <type>  <mnt_flags and options>                                                 <fs_mgr_flags>
/dev/block/mmcblk0p15  /system      ext4    noatime,noauto_da_alloc,nodiratime,barrier=0,ro                         wait
/dev/block/mmcblk0p16  /cache       ext4    noatime,noauto_da_alloc,nosuid,nodev,nodiratime,barrier=0,errors=panic  wait,check
/dev/block/mmcblk0p17  /data        ext4    noatime,noauto_da_alloc,nosuid,nodev,nodiratime,barrier=0,errors=panic  wait,check,encryptable=footer,length=-16384

# RECOVERY
#<src>                 <mnt_point>   <type>  <mnt_flags and options>  <fs_mgr_flags>
/dev/block/mmcblk0p8   /boot         emmc    defaults                 recoveryonly
/dev/block/mmcblk0p13  /recovery     emmc    defaults                 recoveryonly
/dev/block/mmcblk1p1   /external_sd  auto    defaults                 recoveryonly

# VOLD
#<src>                                      <mnt_point>       <type>  <mnt_flags and options>  <fs_mgr_flags>
/devices/platform/msm_sdcc.2/mmc_host/mmc1  /storage/sdcard0  auto    defaults                 voldmanaged=internal:28,nonremovable
/devices/platform/msm_sdcc.4/mmc_host/mmc2  /storage/sdcard1  auto    defaults                 voldmanaged=external:auto
/devices/platform/msm_hsusb_host.0          /storage/usbdisk  auto    defaults                 voldmanaged=usbdisk:auto
